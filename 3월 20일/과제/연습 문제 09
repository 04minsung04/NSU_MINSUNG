package code;

import java.util.Scanner;

public class coding {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int sum = 0;
		for (int i = 0; i < 5; i++) {
			if (i % 3 == 0)
				continue;
			sum += i;
		}
		System.out.printf("%d", sum);
	}
}
